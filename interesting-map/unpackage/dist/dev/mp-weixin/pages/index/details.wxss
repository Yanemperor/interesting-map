
.map {
	width: 100%;
	height: 400px;
	overflow: hidden;
}
.text {
	padding-top: 12px;
	padding-left: 16px;
	padding-right: 16px;
	color: #333333;
	font-size: 16px;
}
.bottom-right {
	display: flex;
	justify-content: flex-end;
}
.right-text {
	color: #333333;
	font-size: 18px;
	font-weight: bold;
	padding-right: 16px;
}
.floatview {
	width: 68px;
	height: 68px;
	position: fixed;
	right: 12px;
	bottom: 20px;
	z-index: 99999;
	background-color: rgba(0, 0, 0, 0);
	border-radius: 0px;
	border: 0rpx solid #ffffff;
}
.floatbtn {
	width: 68px;
	height: 68px;
	background-color: rgba(0, 0, 0, 0);
}

